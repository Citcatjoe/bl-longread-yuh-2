	{% raw %}  
	<script id="card-template" type="text/x-handlebars-template">
		{{#each card}}
		<div id="{{@index}}" class="{{class}}">
			<img src="img/this/triangle.svg" alt="" class="absolute triangle left-7 bottom-0 translate-y-full z-10">

			
		
			{{#switch hasImg}}
				{{#case null}}
					<h2 class="absolute top-3 md:top-5 left-4 md:left-7">{{label1}}</h2>
					<span class="label2 absolute bottom-4 md:bottom-7 left-4 md:left-7 right-4 md:right-7 {{label2FontSize}}">{{label2}}</span>
				{{/case}}
				{{#case 'right'}}
					<div class="bg w-1/4 h-full bg-amber-200 rounded-2xl md:rounded-3xl border-4 md:border-8 border-white float-right" style="background:url('img/this/{{img}}') 30% 200% no-repeat, linear-gradient(55deg, {{bgColor}}, {{lightenColor bgColor}}); background-size: 200% auto"></div>
					<h2 class="absolute w-3/4 pr-8 top-3 md:top-5 left-4 md:left-7">{{label1}}</h2>
					<span class="label2  block absolute w-3/4 pr-12 bottom-4 md:bottom-7 left-4 md:left-7 {{label2FontSize}}">{{label2}}</span>
				{{/case}}
				{{#case 'top'}} 
					<div class="bg w-full h-1/2 rounded-2xl md:rounded-3xl border-4 md:border-8 border-white" style="background:url('img/this/{{img}}') 115% center no-repeat, linear-gradient(60deg, {{bgColor}}, {{lightenColor bgColor}}); background-size: auto 120%"></div>
					<h2 class="absolute top-3 md:top-5 left-4 md:left-7 text-white">{{label1}}</h2>
					<span class="label2 absolute bottom-4 md:bottom-7 left-4 md:left-7 right-4 md:right-7 {{label2FontSize}}">{{label2}}</span>
				{{/case}} 
				{{#case 'full'}}
					<div class="bg w-full h-full bg-rose-800 rounded-2xl md:rounded-3xl border-4 md:border-8 border-white" style="background-image:url('img/this/{{img}}'),linear-gradient(60deg, {{bgColor}}, {{lightenColor bgColor}}); background-position: 0% center; background-size: 140%; background-repeat: no-repeat;"></div>
					<div class="bg gradient absolute top-1 md:top-2 right-1 md:right-2 bottom-1 md:bottom-2 left-1 md:left-2 rounded-xl md:rounded-2xl"></div>
					<h2 class="absolute top-3 md:top-5 left-4 md:left-7 text-white z-10">{{label1}}</h2>
					<span class="label2 absolute bottom-4 md:bottom-7 left-4 md:left-7 right-4 md:right-7 {{label2FontSize}} text-white z-10">{{label2}}</span>
				{{/case}} 
				{{else}}
					<p>The image position is not specified.</p>
			{{/switch}}
			
		</div>
		{{/each}}
	</script>
	{% endraw %}
